{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },

    {
      "src": "/(.*\\.(js|css|png|jpg|svg|ico))",
      "dest": "/dist/$1"
    },

    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ]
}